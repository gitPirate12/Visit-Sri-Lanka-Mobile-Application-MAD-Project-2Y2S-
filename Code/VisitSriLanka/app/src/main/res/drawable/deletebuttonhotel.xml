<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <!--stroke-> Little border around elements-->
    <stroke
        android:width="1dp"
        android:color="#000000"/>

    <!--solid-> is background of the button-->
    <solid
        android:color="#f44336"
        />

    <!--corners-> is corner of the button-->
    <corners
        android:radius="30dp"
        />
</shape>